# Environment Configuration for Hospital Crowd Analysis System
# Copy this file to .env and fill in your actual values

# Database Configuration (Supabase PostgreSQL)
# Get these from: Supabase Dashboard > Project Settings > Database
DB_USER=postgres
DB_PASSWORD=your_database_password_here
DB_HOST=db.your_project_ref.supabase.co
DB_PORT=5432
DB_NAME=postgres

# Google Gemini AI API Key
# Get your key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Gemini Model Configuration (optional)
GEMINI_MODEL=gemini-1.5-flash  # Options: gemini-1.5-flash, gemini-1.5-pro
GEMINI_TEMPERATURE=0.7  # 0.0 (deterministic) to 1.0 (creative)
GEMINI_MAX_TOKENS=2048

# Application Configuration
DEBUG=False
LOG_LEVEL=INFO

# Video Processing Configuration
DEFAULT_CONFIDENCE_THRESHOLD=0.5
DEFAULT_FRAME_SAMPLE_RATE=30
